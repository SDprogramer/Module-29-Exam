   "source": [
    "def fact(no):\n",
    "    i = 1\n",
    "    f = []\n",
    "    while i <= no:\n",
    "        if no % i == 0:\n",
    "            f.append(i)\n",
    "        i = i + 1\n",
    "    return f\n",
    "\n",
    "no = int(input(\"Enter a number = \"))\n",
    "\n",
    "factors = fact(no)\n",
    "\n",
    "print(f\"The factors of {no} are = {factors}\")"
   ]
